<jxb:bindings
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
        version="2.1">

    <jxb:bindings schemaLocation="generated/employee.xsd">
        <jxb:bindings node="//xs:complexType[@name='employee']">
            <jxb:class ref="testdomain.employee.Employee"/>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='address']">
            <jxb:class ref="testdomain.employee.Address"/>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='phone']">
            <jxb:class ref="testdomain.employee.Phone"/>
        </jxb:bindings>
    </jxb:bindings>

    <jxb:bindings schemaLocation="generated/zoo.xsd">
        <jxb:bindings node="//xs:complexType[@name='zoo']">
            <jxb:class ref="testdomain.zoo.Zoo"/>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='animal']">
            <jxb:class ref="testdomain.zoo.Animal"/>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='dolphin']">
            <jxb:class ref="testdomain.zoo.Dolphin"/>
        </jxb:bindings>
        <jxb:bindings node="//xs:complexType[@name='penguin']">
            <jxb:class ref="testdomain.zoo.Penguin"/>
        </jxb:bindings>

    </jxb:bindings>

</jxb:bindings>
